[project]
	description = "NFS-Ganesha is an NFSv3,v4,v4.1 fileserver that runs in user mode on most UNIX/Linux systems "
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Frank Filz (ffilz)
[access "refs/for/refs/*"]
	read = group nfs-ganesha
	read = group user/Frank Filz (ffilz)
	push = group nfs-ganesha
	push = group user/Frank Filz (ffilz)
	label-Code-Review = -2..+2 group user/Frank Filz (ffilz)
	label-Verified = -1..+1 group nfs-ganesha
	label-Verified = -1..+1 group user/Frank Filz (ffilz)
	rebase = group user/Frank Filz (ffilz)
	removeReviewer = group user/Frank Filz (ffilz)
	submit = group user/Frank Filz (ffilz)
[access "refs/heads/*"]
	read = group user/Frank Filz (ffilz)
	create = group user/Frank Filz (ffilz)
	push = +force group user/Frank Filz (ffilz)
	pushMerge = group user/Frank Filz (ffilz)
	label-Verified = -1..+1 group nfs-ganesha
[access "refs/tags/*"]
	push = +force group user/Frank Filz (ffilz)
	createSignedTag = group user/Frank Filz (ffilz)
	createTag = group user/Frank Filz (ffilz)
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[submit]
	action = merge always
[label "Code-Review"]
	function = MaxWithBlock
	value = -2 This shall not be merged
	value = -1 I would prefer this is not merged as is
	value = 0 No score
	value = +1 Looks good to me, but someone else must approve
	value = +2 Looks good to me, approved
	copyAllScoresOnTrivialRebase = true
	defaultValue = 0
[label "Verified"]
	function = MaxWithBlock
	value = -1 Fails
	value = 0 No score
	value = +1 Verified
    	copyAllScoresIfNoCodeChange = true
	defaultValue = 0
[notify "list"]
	email = devel@lists.nfs-ganesha.org
	type = new_changes
[plugin "zuul-results-summary"]
	enabled = false
[plugin "uploadvalidator"]
	blockedContentTypeWhitelist = false
	maxPathLength = 0
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = false
	rejectSymlink = false
	rejectWindowsLineEndings = false
